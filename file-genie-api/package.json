{
  "name": "file-genie-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "node dist/app",
    "dev": "nodemon --verbose",
    "start-dev": "tsc --build && node dist/app",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production"
  },
  "description": "file-genie-api",
  "author": {
    "name": ""
  },
  "main": "app.js",
  "dependencies": {
    "@azure/identity": "^3.3.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/openai": "^1.0.0-beta.5",
    "@azure/search-documents": "^11.3.2",
    "@azure/storage-blob": "^12.15.0",
    "debug": "^2.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.14.0",
    "pug": "^2.0.0-rc.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/express-serve-static-core": "^4.0.50",
    "@types/mime": "^1.3.1",
    "@types/node": "^14.14.7",
    "@types/serve-static": "^1.7.32",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "nodemon": "^3.0.1",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  }
